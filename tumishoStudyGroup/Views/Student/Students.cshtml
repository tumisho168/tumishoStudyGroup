
@model IEnumerable<tumishoStudyGroup.Models.StudentModel>

@{
    ViewBag.Title = "Student People";
}


<h2>Students Information</h2>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentEmail)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentSurname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentEmail)
            </td>
        </tr>
    }
</table>
